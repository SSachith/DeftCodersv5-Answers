#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a=0,e=0,o=0,u=0,I=0;
    char x[40];
    gets(x);
    for(int i=0;i<strlen(x);i++){
        if(x[i]=='a' || x[i]=='A' ){
            a++;
        }
        if(x[i]=='e'|| x[i]=='E' ){
            e++;
        }
        if(x[i]=='i'|| x[i]=='I'){
            I++;
        }
        if(x[i]=='o'|| x[i]=='O'){
            o++;
        }
        if(x[i]=='u'|| x[i]=='U'){
            u++;
        }
       
        
    }
    
   printf("a - %d\n",a);
   printf("e - %d\n",e);
   printf("i - %d\n",I);
   printf("o - %d\n",o);
   printf("u - %d",u);
    
    
    return 0;
}
